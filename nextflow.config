profiles {
    test {
        params {
            outdir = "${baseDir}/test_data/output"
            input_bam = "${baseDir}/test_data/input/test_data.bam"
            num_repeats = "5"
            num_reads = "300000,200000,100000"
        }
    }

    pbs {
        process.executor = 'pbs'
    }
}

dag {
    enabled = false
    file = "$baseDir/dag.svg"
}
